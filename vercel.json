{
    "version": 2,
    "name": "counterfactual-frontend",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/guide/(.*)",
            "dest": "/guide/$1"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "REACT_APP_API_URL": "@counterfactual-api-url"
    },
    "build": {
        "env": {
            "REACT_APP_API_URL": "@counterfactual-api-url"
        }
    }
}